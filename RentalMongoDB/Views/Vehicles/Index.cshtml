@model IEnumerable<RentalMongoDB.Models.VehicleModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Index")
        </th>
        <th>
            @Html.DisplayName("Plate Number")
        </th>
        <th>
            @Html.DisplayName("Tank Capacity")
        </th>
        <th>
            @Html.DisplayName("Brand")
        </th>
        <th>
            @Html.DisplayName("Style")
        </th>
        <th>
            @Html.DisplayName("Model")
        </th>
        <th>
            @Html.DisplayName("Color")
        </th>
        <th>
            @Html.DisplayName("Horse Power")
        </th>
        <th>
            @Html.DisplayName("Fuel")
        </th>
        <th>
            @Html.DisplayName("Transmition")
        </th>
        <th>
            @Html.DisplayName("Year")
        </th>
        <th>
            @Html.DisplayName("Extras")
        </th>
        <th>
            @Html.DisplayName("Passenger Capacity")
        </th>
        <th>
            @Html.DisplayName("Rental Price")
        </th>
        <th>
            @Html.DisplayName("State")
        </th>
        <th></th>
    </tr>
@{ int i = 0; }
@foreach (var item in Model) {
    <tr>
        <td>
           @(i = i + 1)
            
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Plate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TankCapacity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Brand)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Style)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Model)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Color)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HorsePower)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fuel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Transmition)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Extras)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PassengerCap)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RentalPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { @id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { @id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { @id = item.Id })
        </td>
    </tr>
}

</table>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
